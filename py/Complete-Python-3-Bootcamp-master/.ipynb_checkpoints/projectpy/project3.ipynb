   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of 5 number is : 15\n"
     ]
    }
   ],
   "source": [
    "no = int(input(\"Enter number : \"))\n",
    "num = no + 1\n",
    "sum = 0\n",
    "for i in range (0,num):\n",
    "    sum += int(i)\n",
    "print(\"Sum of\",no,\"number is :\",sum)"
   ]
  }
